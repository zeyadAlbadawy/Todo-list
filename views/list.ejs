<%- include("header"); -%>
  <div id="heading" class="box">
    <h1> <%= listTitle %> </h1>
  </div>

  <div class="box">
    <% for(var i = 0 ; i < newItem.length ; i++){ %>
    <div class="item">
      <input type="checkbox" name="" value="">
      <p class="item"><%= newItem[i] %></p>
    </div>
    <%  } %>

    <form class="item" action="/" method="post">
      <input type="text" name="newListItem" value="" autocomplete="off" placeholder="New Item">
      <button type="submit" name="button" value= <%= listTitle %> >+</button>

    </form>

  </div>
<%- include("footer"); -%>
